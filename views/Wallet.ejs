        <div class="container-fluid" style="margin-top:50px; min-height:100vh; background:#f7f8ff">
            <div class="position-fixed topnavbar bg-primary">
                <div class="navlogo w-100 ">
                    <!-- <a href="#" onclick="location.href = document.referrer; return false;"><img src="img/UI/icons8-back-50.png" style="cursor: pointer;" alt="" srcset=""></a> -->
                    <span>Wallet</span>
                </div>
                <!-- <div class="content-right align-middle">
                    <i class="fa fa-bell" aria-hidden="true"></i>
                </div> -->
            </div><br>
            <div class="text-white rounded-3 p-2 text-center bg-primary">
                <img height="60px" class="me-2" src="data:image/png;base64,">
                <h1 class="mt-2 ms-2">₹ <% da %></h1>
                <div class="d-flex justify-content-center">
                    Total Balance
                </div>
            </div>
            <div class="d-flex justify-content-between text-center">
                <div class="w-50 pe-2">
                    <div class="bg-white shadow-sm rounded-3 mt-3 p-3">
                        <h1 style="color: #888;">₹ <% loginuser.WalletAmount.toFixed(2) %></h1>
                        <span>Main Wallet</span>
                    </div>
                </div>
                <div class="w-50 ps-2">
                    <div class="bg-white shadow-sm rounded-3 mt-3 p-3">
                        <h1 style="color: #888;">₹ <% loginuser.eWalletAmount.toFixed(2) %></h1>
                        <span>e-Wallet</span>
                    </div>
                </div>
            </div>
                <div class="d-flex flex-column mt-3">
                    <div class="d-flex text-center">
                        <div class="w-25 bg-white p-2 rounded-3 shadow-sm ms-0 m-1" onclick="location.href='recharge'">
                            <img width="40px" class="mb-1" src="data:image/png;base64," alt="">
                            <p class="m-0">Make Recharge</p>
                        </div>
                        <div class="w-25 bg-white p-2 rounded-3 shadow-sm m-1" onclick="location.href='withdraw'">
                            <img width="40px" class="mb-1" src="data:image/png;base64," alt="">
                            <p class="m-0">Request Withdraw</p>
                        </div>
                        <div class="w-25 bg-white p-2 rounded-3 shadow-sm m-1">
                            <img width="40px" class="mb-1" src="data:image/png;base64," alt="">
                            <p class="m-0">Recharge History</p>
                        </div>
                        <div class="w-25 bg-white p-2 rounded-3 shadow-sm m-1 me-0">
                            <img alt="" width="40px" class="mb-1" src="data:image/png;base64,">
                            <p class="m-0">Withdrawal History</p>
                        </div>
                    </div>
                </div>
